{
  "name": "supra-l1-sdk",
  "version": "4.0.0",
  "description": "Typescript SDK for Supra",
  "engines": {
    "node": ">=11.0.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:clean && npm run _build:node && npm run _build:browser",
    "build:clean": "rm -rf dist",
    "_build:browser": "tsup --platform browser --format iife --global-name supraSDK --minify",
    "_build:node": "tsup --platform node --format cjs,esm --dts",
    "docs": "npx typedoc",
    "test": "ts-node ./src/example.ts"
  },
  "author": "",
  "keywords": [
    "supra-sdk",
    "supra-l1-sdk",
    "supra-ts-sdk"
  ],
  "license": "ISC",
  "dependencies": {
    "@ethersproject/keccak256": "^5.7.0",
    "aptos": "^1.21.0",
    "js-sha3": "^0.9.3"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  }
}
